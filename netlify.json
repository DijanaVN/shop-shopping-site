{
  "redirects": [
    {
      "from": "/*",
      "to": "/client/dist/index.html",
      "status": 200
    },
    {
      "from": "/api/*",
      "to": "/api/:splat",
      "status": 200
    }
  ],
  "build": {
    "command": "npm run build",
    "functions": "server/functions",
    "publish": "client/dist",
    "base": "/"
  }
}
