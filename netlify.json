{
  "redirects": [
    {
      "from": "/*",
      "to": "/client",
      "status": 200
    },
    {
      "from": "/api/*",
      "to": "/api/:splat",
      "status": 200
    }
  ],
  "build": {
    "command": "cd client && npm run build",
    "functions": "server/functions",
    "publish": "client",
    "base": "/"
  }
}
