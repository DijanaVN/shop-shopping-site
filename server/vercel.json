{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    },
    {
      "src": "./api/handlerFunction.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/api/handlerFunction",
      "dest": "/api/handlerFunction.js"
    }
  ]
}
